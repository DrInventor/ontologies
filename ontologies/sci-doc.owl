        <rdfs:range rdf:resource="&sci-doc;ScientificDiscourse"/>
        <rdfs:subPropertyOf rdf:resource="&cito;cites"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </ObjectProperty>
    


    <!-- http://purl.org/spar/cito/parodies -->

    <ObjectProperty rdf:about="&cito;parodies">
        <rdfs:label xml:lang="en">parodies</rdfs:label>
        <rdfs:comment xml:lang="en">The citing entity imitates the characteristic style or content of the cited entity for comic effect, usually without explicit citation.</rdfs:comment>
        <rdfs:range rdf:resource="&sci-doc;ScientificDiscourse"/>
        <rdfs:subPropertyOf rdf:resource="&cito;cites"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </ObjectProperty>
    


    <!-- http://purl.org/spar/cito/plagiarizes -->

    <ObjectProperty rdf:about="&cito;plagiarizes">
        <rdfs:label xml:lang="en">plagiarizes</rdfs:label>
        <rdfs:comment xml:lang="en">The author of the citing entity plagiarizes the cited entity, by including textual or other elements from the cited entity without formal acknowledgement of their source.  The citing entity thus contains no explicit citation of the cited entity, according to the norms of scholarly practice, but cites it implicitly.</rdfs:comment>
        <rdfs:range rdf:resource="&sci-doc;ScientificDiscourse"/>
