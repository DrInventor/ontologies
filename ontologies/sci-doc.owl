        <rdfs:range rdf:resource="&sci-doc;ScientificDiscourse"/>
        <rdfs:subPropertyOf rdf:resource="&cito;cites"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </ObjectProperty>
    


    <!-- http://purl.org/spar/cito/obtainsSupportFrom -->

    <ObjectProperty rdf:about="&cito;obtainsSupportFrom">
        <rdfs:label xml:lang="en">obtains support from</rdfs:label>
